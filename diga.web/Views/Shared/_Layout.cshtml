                            <li class="nav-item"><a href="~/home/contacts" class="nav-link black p-nav">@CultureHelper.GetTranslation("contacts", Context.Session.GetString("SiteLanguage"))</a></li>
                            <li class="nav-item">
                                <span class="nav-link dropdown-toggle black" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
