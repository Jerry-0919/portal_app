    <script>function validateForm(data) {

            var form = document.getElementById(data.formId);
            form.addEventListener('submit', (event) => {
                validateWholeForm(data);
                event.preventDefault();
            });

            form.addEventListener('keypress', function (e) {
                if (e.key === 'Enter') {
                    validateWholeForm(data);
                }
            });

            var inputs = Array.from(document.querySelectorAll('input'));
            inputs.forEach(input => {
                input.addEventListener("focus", (event) => {
                    input.classList.remove(data.inputErrorClass);
                    // form.classList.remove(data.formInvalidClass);
                    // form.classList.remove(data.formValidClass);

                });
                input.addEventListener("blur", (event) => {
                    validateOneInput(input, data.inputErrorClass);

                });

            });
        }

        function validateWholeForm(data) {
            var isFormValid = true;
            var form = document.getElementById(data.formId);
            var inputs = Array.from(document.querySelectorAll('input'));
            for (let input of inputs) {
                var isInputValid = validateOneInput(input, data.inputErrorClass);
                if (isInputValid === false) {
                    isFormValid = false;
                    break;
                }
            }
            if (isFormValid === false) {
                form.classList.remove(data.formValidClass);
                form.classList.add(data.formInvalidClass);
            } else {
                form.classList.remove(data.formInvalidClass);
                form.classList.add(data.formValidClass);
            }
        }

        function validateOneInput(input, inputErrorClass) {
            var isInputValid = true;
            var validator = input.dataset.validator;
            var required = input.dataset.required;


            if (input.value !== "") {
                switch (validator) {
                    case "letters":
                        var reg = /^[a-zР°-СЏ]+$/i;
                        if (input.value === '') {
                            isInputValid = false;
                            break;
                        }
                        if (input.value.match(reg) === null) {
                            isInputValid = false;
                        }
                        break;
                    case "regexp":
                        var reg = input.dataset.validatorPattern;
                        if (input.value.match(reg) === null) {
                            isInputValid = false;
                        }
                        break;

                }
            } else {
                if (required !== undefined) {
                    isInputValid = false;
                }
            }

            if (isInputValid === false) {
                input.classList.add(inputErrorClass);
            } else {
                input.classList.remove(inputErrorClass);

            }

            return isInputValid;
        }</script>

