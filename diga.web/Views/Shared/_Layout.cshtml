    <script type="text/javascript">$(document).ready(function () {

            $("#domainval").on("change paste keyup", function () {
                $("#domain_spinner").show();
                $("#domainIsBusy").hide();
                $("#form_submit").prop('disabled', true);
                $.get("/request/CheckDomain?domain=" + $(this).val(), function (data, status) {
                    if (data === false) {
                        $("#domain_spinner").hide();
                        $("#domainIsBusy").show();
                        $("#domainval").parent().removeClass('diga-input-valid');
                        $("#domainval").parent().addClass('diga-input-invalid');
                        $("#domainval").removeClass('is-valid');
                    }
                    else {
                        $("#domain_spinner").hide();
                        $("#domainIsBusy").hide();
                        $("#domainval").parent().addClass('diga-input-valid');
                        $("#domainval").parent().removeClass('diga-input-invalid');
                        $("#domainval").addClass('is-valid');
                    }
                    $("#form_submit").prop('disabled', false);
                });

            });


