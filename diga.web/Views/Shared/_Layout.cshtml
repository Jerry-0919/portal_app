    <div class="modal fade" id="contactUsModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <form id="contactUsForm">
                <div class="modal-content">
                    <div class="modal-body p-5 text-center">
                        <span type="button" class="close" data-dismiss="modal" aria-label="Close" aria-hidden="true"><img src="~/Assets/img/close.png" /></span>

                        <p class="h3 blue mb-4">@CultureHelper.GetTranslation("write_to_us", Context.Session.GetString("SiteLanguage"))</p>
                        <p class="t2"> @CultureHelper.GetTranslation("write_us_text", Context.Session.GetString("SiteLanguage"))</p>
                        <p class="t4 form__success-msg">@CultureHelper.GetTranslation("your_request_was_successfully_sent", Context.Session.GetString("SiteLanguage"))</p>
                        <p class="t4 form__error-msg">@CultureHelper.GetTranslation("form_error", Context.Session.GetString("SiteLanguage"))</p>
                        <div class="form-group">
                            <input required aria-required="true" data-required data-validator="letters" type="text" class="form-control" id="contact_name" placeholder="@CultureHelper.GetTranslation("name", Context.Session.GetString("SiteLanguage")) *">
                        </div>
                        <div class="form-group">
                            <input required aria-required="true"  type="tel" class="form-control" id="contact_phone" placeholder="@CultureHelper.GetTranslation("telephone", Context.Session.GetString("SiteLanguage")) *">
                        </div>
                        <div class="form-group">
                            <input required aria-required="true" data-validator="regexp" data-validator-pattern="/^[a-z0-9._%+-]+@@\[a-z0-9.-]+\.[a-z]{2,4}$/" type="email" class="form-control" id="contact_email" placeholder="Email *">
                        </div>
                        <div class="form-group text-left">
                            <textarea aria-describedby="contact_textarea" type="text" class="form-control" id="contact_message" rows="8" placeholder="@CultureHelper.GetTranslation("your_message", Context.Session.GetString("SiteLanguage"))"></textarea>
                            <small id="contact_textarea" class="form-text fs-13 text-muted">@CultureHelper.GetTranslation("by_sending_message", Context.Session.GetString("SiteLanguage"))</small>
                        </div>
                        <button type="submit" class="mt-4 mx-auto btn-orange fs-18">@CultureHelper.GetTranslation("send", Context.Session.GetString("SiteLanguage"))</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
